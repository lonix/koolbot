{
  "name": "koolbot",
  "version": "1.0.0",
  "description": "A Discord bot with dynamic voice channels and time tracking",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate-oauth": "ts-node scripts/generate-oauth-url.ts"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "axios": "^1.6.7",
    "mongoose": "^8.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/sqlite3": "^3.1.11",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  }
}
